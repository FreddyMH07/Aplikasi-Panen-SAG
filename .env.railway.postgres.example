APP_ENV=local
APP_DEBUG=true
APP_URL=http://localhost
APP_TIMEZONE=Asia/Jakarta

# Database Configuration (example only)
DB_CONNECTION=pgsql
# For local development, you can point to Railway Public URL or your local Postgres
DATABASE_URL=postgresql://postgres:password@127.0.0.1:5432/railway?sslmode=prefer
DB_SSLMODE=prefer

# Example PG* variables for local dev
PGHOST=127.0.0.1
PGPORT=5432
PGDATABASE=railway
PGUSER=postgres
PGPASSWORD=password
PGSSLMODE=prefer

# Public URL (useful when connecting from local dev tools like PGAdmin)
DATABASE_PUBLIC_URL=postgresql://postgres:password@localhost:5432/railway

# Cache & Session\SESSION_DRIVER=file
CACHE_STORE=file
