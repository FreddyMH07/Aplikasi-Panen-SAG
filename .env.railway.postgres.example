APP_NAME="Sistem Panen Sawit"
APP_ENV=production
APP_DEBUG=false
APP_KEY=
APP_URL=https://your-app.railway.app

LOG_CHANNEL=stderr
LOG_LEVEL=info

DB_CONNECTION=pgsql
# Jika ingin pakai private network (disarankan): gunakan RAILWAY_PRIVATE_DOMAIN
# Jika ingin pakai host public Postgres (xxxx.up.railway.app), isi manual di DB_HOST via Variables.
DB_HOST=${RAILWAY_PRIVATE_DOMAIN}
DB_PORT=${PGPORT:-5432}
DB_DATABASE=${PGDATABASE:-${POSTGRES_DB}}
DB_USERNAME=${PGUSER:-${POSTGRES_USER}}
DB_PASSWORD=${PGPASSWORD:-${POSTGRES_PASSWORD}}
DB_CHARSET=utf8
# Mode SSL: internal host biasanya cukup prefer; public host gunakan require
PGSSLMODE=prefer

# (Opsional) Bisa juga gunakan single DATABASE_URL (Laravel >=10.14)
# DATABASE_URL=pgsql://${PGUSER}:${PGPASSWORD}@${RAILWAY_PRIVATE_DOMAIN}:5432/${PGDATABASE}?sslmode=prefer

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_SECURE_COOKIE=true

MAIL_MAILER=log
