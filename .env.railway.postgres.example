APP_NAME="Sistem Panen Sawit"
APP_ENV=production
APP_DEBUG=false
APP_KEY=
APP_URL=https://your-app.railway.app

LOG_CHANNEL=stderr
LOG_LEVEL=info

DB_CONNECTION=pgsql
# Ganti nilai di bawah via Railway Variables pada runtime; ini hanya placeholder lokal.
DB_HOST=postgres-production-f302.up.railway.app
DB_PORT=5432
DB_DATABASE=railway
DB_USERNAME=postgres
DB_PASSWORD=change_me
DB_CHARSET=utf8
PGSSLMODE=require

# Alternatif (gunakan salah satu, jika pakai ini boleh hapus DB_* di atas):
# DATABASE_URL=pgsql://postgres:change_me@postgres-production-f302.up.railway.app:5432/railway?sslmode=require

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_SECURE_COOKIE=true

MAIL_MAILER=log

# Seeder user domain & optional passwords
USER_EMAIL_DOMAIN=sahabatagro.co.id
# (Opsional) Override default seeder passwords; jika tidak diisi pakai default di seeder
# ADMIN_PASSWORD=
# MANAGER_PASSWORD=
# OPERATOR_PASSWORD=
