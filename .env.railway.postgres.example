APP_NAME="Sistem Panen Sawit"
APP_ENV=production
APP_DEBUG=false
APP_KEY=
APP_URL=https://your-app.railway.app

LOG_CHANNEL=stderr
LOG_LEVEL=info

DB_CONNECTION=pgsql
# Rekomendasi: gunakan hanya DATABASE_URL (internal private domain) di Railway Variables
# Ganti <RAILWAY_PRIVATE_DOMAIN> dengan nilai sebenarnya yang tampil di service Postgres (contoh: postgres-prd-abcd.up.railway.internal)
DATABASE_URL=pgsql://postgres:PFXmmrFdlJiIbSKgjHOAOXaAkLPTLVar@<RAILWAY_PRIVATE_DOMAIN>:5432/railway?sslmode=prefer
PGSSLMODE=prefer

# Jika lebih suka pecah variabel (JANGAN gabung dengan DATABASE_URL, pilih salah satu) hapus DATABASE_URL lalu isi yang di bawah:
# DB_HOST=<RAILWAY_PRIVATE_DOMAIN>
# DB_PORT=5432
# DB_DATABASE=railway
# DB_USERNAME=postgres
# DB_PASSWORD=PFXmmrFdlJiIbSKgjHOAOXaAkLPTLVar
# DB_CHARSET=utf8
# PG_SCHEMA=public

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_SECURE_COOKIE=true

MAIL_MAILER=log

# Seeder user domain & optional passwords
USER_EMAIL_DOMAIN=sahabatagro.co.id
# (Opsional) Override default seeder passwords; jika tidak diisi pakai default di seeder
# ADMIN_PASSWORD=
# MANAGER_PASSWORD=
# OPERATOR_PASSWORD=
